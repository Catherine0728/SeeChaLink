<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="fill" format="boolean" />
    <attr name="Paint_Width" format="integer" />
    <attr name="Paint_Color" format="integer" />
    <attr name="Inside_Interval" format="integer" />
    <attr name="Paint_BG_Color" format="integer" />
    <attr name="value_text_color" format="integer" />
    <attr name="text_size" format="integer" />
    <attr name="value_text_size" format="integer" />
    <attr name="count" format="integer" />

    <declare-styleable name="LockPatternView">
        <attr name="aspect" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <!-- 圆角图片 -->
    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>

    <!-- 滑动按钮需要的 -->
    <declare-styleable name="SlideSwitch">

        <!-- 按钮底层背景 -->
        <attr name="background" format="reference" />
        <!-- 上层按钮背景 -->
        <attr name="btn_background" format="reference" />
        <!-- 控件width -->
        <attr name="mwidth" format="dimension" />
        <!-- 控件height -->
        <attr name="mheight" format="dimension" />
        <!-- 控件上可拖动按钮width -->
        <attr name="mbt_width" format="dimension" />
        <!-- 控件上可拖动按钮height -->
        <attr name="mbt_height" format="dimension" />
    </declare-styleable>

</resources>